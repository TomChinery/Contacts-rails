<h1>Home#index</h1>
<p>Find me in app/views/home/index.html.erb</p>

<% if current_user %>

  <% @contacts.each do |contact| %>
    <div class="contact">
      <label>Name: <span><%= contact[:name] %></span></label>
      <label>Email: <span><%= contact[:email] %></span></label>
      <label>Phone: <span><%= contact[:phone] %></span></label>
    </div>
  <% end %>

<% end %>
